    "ejs": "^3.1.2",
    "handlebars": "^4.7.6",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "moment": "^2.27.0",
    "nest-access-control": "^2.0.2",
    "nestjs-command": "^1.5.0",
    "nodemailer": "^6.4.6",
