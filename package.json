    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@types/yargs": "^15.0.5",
